<?xml version="1.0" encoding="utf-8"?>
<!-- infrastructure.command -->
<openerp>
<data>

<!-- SEARCH VIEW -->
<record id="view_infrastructure_command_filter" model="ir.ui.view">
    <field name="name">infrastructure.command.select</field>
    <field name="model">infrastructure.command</field>
    <field name="arch" type="xml">
        <search string="command">
            <field name="name"/>
            <field name="command"/>
        </search>
    </field>
</record>

<!-- FORMVIEW -->
<record id="view_infrastructure_command_form" model="ir.ui.view">
    <field name="name">infrastructure.command.form</field>
    <field name="model">infrastructure.command</field>
    <field name="arch" type="xml">
        <form string="command">
            <header>
            </header>
            <sheet string="command">
             <group>
                <field name="name"/>
                <field name="command"/>
             </group>
            </sheet>
        </form>
    </field>
</record>

<!-- TREEVIEW -->
<record id="view_infrastructure_command_tree" model="ir.ui.view">
    <field name="name">infrastructure.command.tree</field>
    <field name="model">infrastructure.command</field>
    <field name="arch" type="xml">
        <tree string="command">
            <field name="sequence" widget="handle"/>
            <field name="name"/>
            <field name="command"/>
        </tree>
    </field>
</record>

</data>
</openerp>
