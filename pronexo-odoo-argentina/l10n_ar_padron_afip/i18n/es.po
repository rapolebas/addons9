#. module: l10n_ar_padron_afip
#: model:ir.ui.view,arch_db:l10n_ar_padron_afip.view_base_partner_update_from_padron_form
msgid ""
"<?xml version=\"1.0\"?>\n"
"<form string=\"Update From Padron AFIP\">\n"
"                    <field name=\"state\" invisible=\"1\"/>\n"
"                    <header>\n"
"                        <button name=\"update_selection\" string=\"Update Selection\" class=\"oe_highlight\" type=\"object\" attrs=\"{'invisible': [('state', 'in', ('option', 'finished' ))]}\"/>\n"
"                        <button name=\"next_cb\" string=\"Skip these partner\" type=\"object\" class=\"oe_link\" attrs=\"{'invisible': [('state', '!=', 'selection')]}\"/>\n"
"                        <button name=\"start_process_cb\" string=\"Update with Manual Check\" type=\"object\" class=\"oe_highlight\" attrs=\"{'invisible': [('state', '!=', 'option')]}\"/>\n"
"                        <button name=\"automatic_process_cb\" string=\"Update Automatically\" type=\"object\" class=\"oe_highlight\" confirm=\"Are you sure to execute the automatic update of your partners ?\" attrs=\"{'invisible': [('state', '!=', 'option')]}\"/>\n"
"                        <span class=\"or_cancel\" attrs=\"{'invisible': [('state', '=', 'finished')]} \">or\n"
"                            <button string=\"Cancel\" class=\"oe_link oe_inline\" special=\"cancel\"/>\n"
"                        </span>\n"
"                        <span class=\"or_cancel\" attrs=\"{'invisible': [('state', '!=', 'finished')]} \">\n"
"                            <button string=\"Close\" class=\"oe_link oe_inline\" special=\"cancel\"/>\n"
"                        </span>\n"
"                    </header>\n"
"                    <sheet>\n"
"                        <group attrs=\"{'invisible': [('state', '!=', 'finished')]}\" col=\"1\">\n"
"                            <h2>There are no more partners to update for this request...</h2>\n"
"                            <!-- <button name=\"263\" string=\"Update the other partners\" class=\"oe_highlight\"\n"
"                            type=\"action\"/> -->\n"
"                        </group>\n"
"                        <p class=\"oe_grey\" attrs=\"{'invisible': [('state', '!=', ('option'))]}\">\n"
"                            Only Partners with cuit are going to be updated.<br/>\n"
"                            Select the list of fields you want to update.\n"
"                        </p>\n"
"                        <group attrs=\"{'invisible': [('state', 'not in', ('option',))]}\">\n"
"                            <field name=\"field_to_update_ids\" widget=\"many2many_tags\" options=\"{'no_create': True}\"/>\n"
"                            <field name=\"update_constancia\"/>\n"
"                            <field name=\"title_case\"/>\n"
"                        </group>\n"
"                        <group attrs=\"{'invisible': [('state', 'in', ('option', 'finished'))]}\" col=\"1\">\n"
"                            <field name=\"partner_id\" attrs=\"{'required': [('state', '=', 'selection')]}\"/>\n"
"                            <field name=\"field_ids\" nolabel=\"1\">\n"
"                                <tree create=\"false\">\n"
"                                    <field name=\"field\"/>\n"
"                                    <field name=\"old_value\"/>\n"
"                                    <field name=\"new_value\"/>\n"
"                                </tree>\n"
"                            </field>\n"
"                        </group>\n"
"                    </sheet>\n"
"                </form>\n"
"            "
msgstr ""
"<?xml version=\"1.0\"?>\n"
"<form string=\"Update From Padron AFIP\">\n"
"                    <field name=\"state\" invisible=\"1\"/>\n"
"                    <header>\n"
"                        <button name=\"update_selection\" string=\"Update Selection\" class=\"oe_highlight\" type=\"object\" attrs=\"{'invisible': [('state', 'in', ('option', 'finished' ))]}\"/>\n"
